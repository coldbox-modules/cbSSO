{
    "name":"cbsso-boxlang",
    "app":{
        "serverHomeDirectory":".engine/boxlang",
        "cfengine":"boxlang@be",
        "libDirs":"./java/cbsso-opensaml/app/build/libs"
    },
    "jvm":{
        "javaVersion":"openjdk21_jdk"
    },
    "web":{
        "http":{
            "port":"9181"
        },
        "rewrites":{
            "enable":"true"
        },
        "webroot":"test-harness",
        "aliases":{
            "/moduleroot/cbSSO":"../"
        }
    },
    "openBrowser":"false",
    "cfconfig":{
        "file":".cfconfig.json"
    },
    "scripts":{
        "onServerInitialInstall":"install bx-compat-cfml,bx-esapi"
    }
}
